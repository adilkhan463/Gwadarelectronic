<%- include('header') %> 
	<section>
		<div class="container">
			<div class="row">
				<%- include('side') %> 
				
				<div class="col-sm-9 padding-right">
					<div class="features_items"><!--features_items-->
						<h2 class="title text-center">Features Itemsx</h2>
						<% students.forEach(student =>{ %>
						<div class="col-sm-4">
							
							<div class="product-image-wrapper">
								
								<div class="single-products">
									<div class='productinfo text-center'>
										<img src="/uploads/<%= student.image %>" alt='' srcset='' width='98px' height='150px'>
										
										<h2><%= student.brand %></h2>
										<h3><%= student.category %></h3>
										<p><%= student.year %></p>
										<p><%= student.descrption %></p>
										<h2>Rs. <%= student.price %></h2>
										
										<a href="#" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Add to cart</a>
									</div>
									<div class="product-overlay">
										
										<div class="overlay-content">
											<h2>Rs. <%= student.price %></h2>
											<h2><%= student.brand %></h2>
											
											<p><%= student.descrption %></p>
											<p><%= student.year %></p>
											<a href="#" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Add to cart</a>
										</div>
									</div>
								</div>
								
							</div>
							
						</div>
						<% }) %>	
					</div><!--features_items-->
				</div>
			</div>
		</div>
	</section>

<%- include('footer') %> 