<%- include('header2') %> 
                <!-- Begin Page Content -->
                <div class="container-fluid">
                    
                  

                    <!-- Page Heading -->
                    <h1 class="h3 mb-4 text-gray-800">Update Student page</h1>
                    <form action="/usercer" method="post">
                        <label class="form-item" for="name">Name:</label>
                        <input class="form-control" type="text"   name="name">
                        <label class="form-item" for="batch">Email:</label>
                        <input type="email" class="form-control" name="email">

                        <label class="form-item" for="batch">Role:</label>
                        <select class="form-control" name="role" id="">
                            <option value="1">user</option>
                            <option value="2">admin</option>
                            <option value="3">super admin</option>
                        </select>

                        <label class="form-item" for="batch">Password:</label>
                        <input type="password" class="form-control"  class="form-control-file"   name="password">
                        
                        <br>
                        <button type="submit" class="btn btn-primary">Create</button>
                    </form>

                </div>
